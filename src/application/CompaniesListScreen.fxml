<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.*?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.CompaniesList">
  <!-- TODO Add Nodes -->
  <bottom>
    <Pane prefHeight="-1.0" prefWidth="400.0" BorderPane.alignment="CENTER" BorderPane.margin="$x1">
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
      </padding>
    </Pane>
  </bottom>
  <center>
    <AnchorPane prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <MenuButton layoutX="55.0" layoutY="90.0" mnemonicParsing="false" text="MenuButton">
          <contextMenu>
            <ContextMenu>
              <items>
                <MenuItem mnemonicParsing="false" text="Unspecified Action" />
                <MenuItem mnemonicParsing="false" text="Unspecified Action" />
              </items>
            </ContextMenu>
          </contextMenu>
          <items>
            <MenuItem mnemonicParsing="false" text="Action 1" />
            <MenuItem mnemonicParsing="false" text="Action 2" />
          </items>
        </MenuButton>
      </children>
      <BorderPane.margin>
        <Insets />
      </BorderPane.margin>
    </AnchorPane>
  </center>
  <top>
    <Pane padding="$x1" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER" BorderPane.margin="$x1">
      <children>
        <Label text="Click on the company or" textFill="#008747">
          <font>
            <Font name="Comic Sans MS" size="16.0" />
          </font>
        </Label>
      </children>
    </Pane>
  </top>
</BorderPane>
